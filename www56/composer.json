{
  "name": "like/exemplo56",
  "description": "Exemplo usando o PHP 5.6 com Phinx.",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.likesistemas.com.br/"
    }
  ],
  "require": {
    "ext-pdo": "*",
    "robmorgan/phinx": "^0.11.7",
    "symfony/yaml": "^3.4",
    "like/exception": "^1.2"
  },
  "scripts": {
    "migrate": "phinx migrate",
    "seed": "phinx seed:run"
  },
  "extra": {
    "cleaner-ignore": {
      "robmorgan/phinx": true
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    }
  }
}
